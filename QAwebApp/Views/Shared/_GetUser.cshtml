@model QAwebApp.Models.FormModel


@using (Html.BeginForm("Assign", "Form", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <div>

        <h4>User Details:</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.User.Name)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.User.Name)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.User.Sid)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.User.Sid)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.User.Email)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.User.Email)
            </dd>
            @Html.HiddenFor(m => m.Id)
            @Html.HiddenFor(m => m.ManagerSid)
        </dl>


    </div>
    <div>
        <input type="submit" value="Save" class="btn btn-default" />
        <a class="btn btn-default" href="@Url.Action("Index", "Form")">Cancel</a>

    </div>
}
